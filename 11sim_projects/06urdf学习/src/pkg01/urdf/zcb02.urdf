<?xml version="1.0"?>
<robot name="myrobot">
  <link name="base_link">
    <visual>
      <geometry>
        <cylinder length="0.6" radius="0.2"/>
      </geometry>
    </visual>
  </link>
  <link name="right_leg">
    <visual>
      <geometry>
        <box size="0.6 0.1 0.2"></box>
      </geometry>
    </visual>
  </link>
  <joint name="base2right_leg" type="fixed">
    <parent link="base_link" />
    <child link="right_leg" />
  </joint>

</robot>
